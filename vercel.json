{
  "name": "pandora-cloud",
  "version": 2,
  "framework": null,
  "builds": [
    {
      "src": "entrypoint.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "entrypoint.py"
    }
  ],
  "env": {
    "PANDORA_SERVER_LISTEN": "false"
  }
}
